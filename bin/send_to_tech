#!/bin/sh

if [ $# -ne 1 ]; then
	echo "[Usage] `basename $0` (file)  : Upload (file) to Tech by FTP."
	exit
fi

if [ ! -f $1 ]; then
	echo "Not found file: $1"
	exit
fi


echo "=== Start FTP ==="
ftp -v -n 10.103.0.1 <<_EOD
user ytanaka kl67u1oz
ls
quit
_EOD
echo "==== End FTP ===="

#put mt_mente_announce_B.sql /home/ytanaka/mt_mente_announce_B.sql
#bye
